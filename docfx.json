{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**/*.csproj"],
          "exclude": [
            "src/**/*.UnitTests.csproj",
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "output": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "artifacts/**"
        ]
      }
    ]
  }
}